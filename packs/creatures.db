ity":"common","custom":""},"rules":[],"slug":"forbidding-ward","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 ally and 1 enemy"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"xF7byexWdlFsn55v","name":"Ghost Sound","type":"spell","img":"systems/pf2e/icons/spells/ghost-sound.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.atlgGNI1E1Ox3O3a"}},"system":{"description":{"value":"<p>You create an auditory illusion of simple sounds that has a maximum volume equal to four normal humans shouting. The sounds emanate from a square you designate within range. You can't create intelligible words or other intricate sounds (such as music).</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> The range increases to 60 feet.</p>\n<p><strong>Heightened (5th)</strong> The range increases to 120 feet.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["auditory","cantrip"],"rarity":"common","custom":""},"rules":[],"slug":"ghost-sound","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"sustained"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightening":{"levels":{"3":{"range":{"value":"60 feet"}},"5":{"range":{"value":"120 feet"}}},"type":"fixed"}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"gF4vztUs6fI0KMUs","name":"Shield","type":"spell","img":"systems/pf2e/icons/spells/shield.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.TVKNbcgTee19PXZR"}},"system":{"description":{"value":"<p>You raise a magical shield of force. This counts as using the Raise a Shield action, giving you a +1 circumstance bonus to AC until the start of your next turn, but it doesn't require a hand to use.</p>\n<p>While the spell is in effect, you can use the Shield Block reaction with your magic shield. The shield has Hardness 5. After you use Shield Block, the spell ends and you can't cast it again for 10 minutes. Unlike a normal Shield Block, you can use the spell's reaction against the magic missile spell.</p>\n<p>Heightening the spell increases the shield's Hardness.</p>\n<p>@Compendium[pf2e.spell-effects.Jemq5UknGdMO7b73]{Spell Effect: Shield}</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> The shield has Hardness 10.</p>\n<p><strong>Heightened (5th)</strong> The shield has Hardness 15.</p>\n<p><strong>Heightened (7th)</strong> The shield has Hardness 20.</p>\n<p><strong>Heightened (9th)</strong> The shield has Hardness 25.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["cantrip","force"],"rarity":"common","custom":""},"rules":[],"slug":"shield","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":false,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"1"},"duration":{"value":"until the start of your next turn"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"SE4DSoF47VpfUPbq","name":"Telekinetic Projectile","type":"spell","img":"systems/pf2e/icons/spells/telekinetic-projectile.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.60sgbuMWN0268dB7"}},"system":{"description":{"value":"<p>You hurl a loose, unattended object that is within range and that has 1 Bulk or less at the target. Make a spell attack roll against the target. If you hit, you deal bludgeoning, piercing, or slashing damage-as appropriate for the object you hurled-equal to 1d6 plus your spellcasting ability modifier. No specific traits or magic properties of the hurled item affect the attack or the damage.</p>\n<hr />\n<p><strong>Critical Success</strong> You deal double damage.</p>\n<p><strong>Success</strong> You deal full damage.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["attack","cantrip"],"rarity":"common","custom":""},"rules":[],"slug":"telekinetic-projectile","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"attack"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":true,"type":{"categories":[],"value":"untyped"},"value":"1d6"}}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightening":{"type":"interval","interval":1,"damage":{"0":"1d6"}}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"NOgNqejNRnFydfga","name":"Color Spray","type":"spell","img":"systems/pf2e/icons/spells/color-spray.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.UKsIOWmMx4hSpafl"}},"system":{"description":{"value":"<p>Swirling colors affect viewers based on their Will saves.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature is @Compendium[pf2e.conditionitems.TkIyaNPgTZFBCCuh]{Dazzled} for 1 round.</p>\n<p><strong>Failure</strong> The creature is @Compendium[pf2e.conditionitems.dfCMdR4wnpbYNTix]{Stunned 1}, @Compendium[pf2e.conditionitems.XgEqL1kFApUbl5Z2]{Blinded} for 1 round, and Dazzled for 1 minute.</p>\n<p><strong>Critical Failure</strong> The creature is Stunned for 1 round and Blinded for 1 minute.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["incapacitation","visual"],"rarity":"common","custom":""},"rules":[],"slug":"color-spray","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":15,"areaType":"cone"},"time":{"value":"2"},"duration":{"value":"1 or more rounds (see below)"},"damage":{"value":{}},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":1},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"15-foot cone"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":1}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"SDwHfqlQpoGdjzup","name":"Illusory Disguise","type":"spell","img":"systems/pf2e/icons/spells/illusory-disguise.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.i35dpZFI7jZcRoBo"}},"system":{"description":{"value":"<p>You create an illusion that causes you to appear as another creature of the same body shape, and with roughly similar height (within 6 inches) and weight (within 50 pounds), as yourself. The disguise is typically good enough to hide your identity, but not to impersonate a specific individual. The spell doesn't change your voice, scent, or mannerisms. You can change the appearance of your clothing and worn items, such as making your armor look like a dress. Held items are unaffected, and any worn item you remove returns to its true appearance.</p>\n<p>Casting illusory disguise counts as setting up a disguise for the @Compendium[pf2e.action-macros.k5nW4jGyXD0Oq9LR]{Impersonate} use of Deception; it ignores any circumstance penalties you might take for disguising yourself as a dissimilar creature, it gives you a +4 status bonus to Deception checks to prevent others from seeing through your disguise, and you add your level even if you're untrained. You can Dismiss this spell.</p>\n<hr />\n<p><strong>Heightened (2nd)</strong> The spell also disguises your voice and scent, and it gains the auditory and olfactory traits.</p>\n<p><strong>Heightened (3rd)</strong> You can appear as any creature of the same size, even a specific individual. You must have seen an individual to take on their appearance. The spell also disguises your voice and scent, and it gains the auditory and olfactory traits.</p>\n<p>@Compendium[pf2e.spell-effects.3Ktyd5F9lOPo4myk]{Spell Effect: Illusory Disguise}</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["visual"],"rarity":"common","custom":""},"rules":[],"slug":"illusory-disguise","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"1 hour"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":1,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":1}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"vk0FMsk6mniBHFuC","name":"True Strike","type":"spell","img":"systems/pf2e/icons/spells/true-strike.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Gb7SeieEvd0pL2Eh"}},"system":{"description":{"value":"<p>A glimpse into the future ensures your next blow strikes true. The next time you make an attack roll before the end of your turn, roll the attack twice and use the better result. The attack ignores circumstance penalties to the attack roll and any flat check required due to the target being @Compendium[pf2e.conditionitems.DmAIPqOBomZ7H95W]{Concealed} or @Compendium[pf2e.conditionitems.iU0fEDdBp3rXpTMC]{Hidden}.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["fortune"],"rarity":"common","custom":""},"rules":[],"slug":"true-strike","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"divination"},"components":{"focus":false,"material":false,"somatic":false,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"1"},"duration":{"value":"until the end of your turn"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":1},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":1}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"POIXVMt4tlv0jExL","name":"Summon Fey","type":"spell","img":"systems/pf2e/icons/spells/summon-fey.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.hs7h8f4Z1ZNdUt3s"}},"system":{"description":{"value":"<p>You conjure a fey to fight for you. This works like summon animal, except you summon a common creature that has the fey trait and whose level is -1.</p>\n<hr />\n<p><strong>Heightened (2nd)</strong> Level 1</p>\n<p><strong>Heightened (3rd)</strong> Level 2</p>\n<p><strong>Heightened (4th)</strong> Level 3</p>\n<p><strong>Heightened (5th)</strong> Level 5</p>\n<p><strong>Heightened (6th)</strong> Level 7</p>\n<p><strong>Heightened (7th)</strong> Level 9</p>\n<p><strong>Heightened (8th)</strong> Level 11</p>\n<p><strong>Heightened (9th)</strong> Level 13</p>\n<p><strong>Heightened (10th)</strong> Level 15</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"summon-fey","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["occult","primal"]},"school":{"value":"conjuration"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"3"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":1},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":1}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"ABBoXnO22lVWAINL","name":"False Life","type":"spell","img":"systems/pf2e/icons/spells/false-life.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.8ViwItUgwT4lOvvb"}},"system":{"description":{"value":"<p>You create a reservoir of vitality from necromantic energy, gaining a number of temporary Hit Points equal to 6 plus your spellcasting ability modifier.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The temporary Hit Points increase by 3.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"false-life","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":2},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"8 hours"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":2},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":2}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"mI4CVh0i6YGN1zPN","name":"Invisibility","type":"spell","img":"systems/pf2e/icons/spells/invisibility.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.XXqE1eY3w3z6xJCB"}},"system":{"description":{"value":"<p>Cloaked in illusion, the target becomes @Compendium[pf2e.conditionitems.zJxUflt9np0q4yML]{Invisible}. This makes it @Compendium[pf2e.conditionitems.VRSef5y1LmL2Hkjf]{Undetected} to all creatures, though the creatures can attempt to find the target, making it @Compendium[pf2e.conditionitems.iU0fEDdBp3rXpTMC]{Hidden} to them instead. If the target uses a hostile action, the spell ends after that hostile action is completed.</p>\n<hr />\n<p><strong>Heightened (4th)</strong> The spell lasts 1 minute, but it doesn't end if the target uses a hostile action.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"invisibility","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":2},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":true,"somatic":true,"verbal":false},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":2,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":2}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"BoBvh7UdZMv9qhVO","name":"Mirror Image","type":"spell","img":"systems/pf2e/icons/spells/mirror-image.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.j8vIoIEWElvpwkcI"}},"system":{"description":{"value":"<p>Three illusory images of you swirl about your space, potentially causing those who attack you to hit one of the images instead of you. Any attack that would hit you has a random chance of hitting one of your images instead of you. If all three images remain, there is a 1 in 4 chance of hitting you (1 on 1d4). With two images remaining, there is a 1 in 3 chance of hitting you (1-2 on 1d6). With only one image, the chances are 1 in 2 (1-3 on 1d6).</p>\n<p>Once an image is hit, it is destroyed. If an attack roll fails to hit your AC but doesn't critically fail, it destroys an image but has no additional effect (even if the attack would normally have an effect on a failure). If an attack roll is a critical success and would hit one of the images, one of the images is destroyed and the attack roll becomes a success against you. Once all the images are destroyed, the spell ends.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["visual"],"rarity":"common","custom":""},"rules":[],"slug":"mirror-image","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":2},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":2},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":2}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"cyJIKbONO5bCeDla","name":"Haste","type":"spell","img":"systems/pf2e/icons/spells/haste.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.o6YCGx4lycsYpww4"}},"system":{"description":{"value":"<p>Magic empowers the target to act faster. It gains the @Compendium[pf2e.conditionitems.nlCjDvLMf2EkV2dl]{Quickened} condition and can use the extra action each round only for Strike and Stride actions.</p>\n<hr />\n<p><strong>Heightened (7th)</strong> You can target up to 6 creatures.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"haste","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":3},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult","primal"]},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":3},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":3},"heightening":{"levels":{"7":{"target":{"value":"6 creatures"}}},"type":"fixed"}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"x768mjGtyxMr2QDS","name":"Invisibility Sphere","type":"spell","img":"systems/pf2e/icons/spells/invisibility-sphere.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.I0j56TNRmGcTyoqJ"}},"system":{"description":{"value":"<p>You and all targets are @Compendium[pf2e.conditionitems.zJxUflt9np0q4yML]{Invisible} except to each other as long as you remain within the spell's area. If a creature made invisible by this spell leaves the spell's area, it becomes visible and remains so even if it returns to the spell's area. If any creature made invisible by this spell uses a hostile action, the spell ends after the hostile action is completed.</p>\n<p>While exploring, it's easy to move together slowly and remain invisible. This is untenable in a battle, however. Once an encounter begins, creatures remain invisible until at most the end of the first round, at which point the spell ends.</p>\n<hr />\n<p><strong>Heightened (5th)</strong> The duration increases to 1 hour.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"invisibility-sphere","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":3},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":true,"somatic":true,"verbal":false},"materials":{"value":""},"target":{"value":"you and any number of creatures in range"},"range":{"value":""},"area":{"value":"10","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":3},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"10-foot emanation"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":3}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"2nJuG8bIutK2CdZl","name":"Vampiric Touch","type":"spell","img":"systems/pf2e/icons/spells/vampiric-touch.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.N1Z1oLPdBxaSgrEE"}},"system":{"description":{"value":"<p>Your touch leeches the lifeblood out of a target to empower yourself. You deal 6d6 negative damage to the target. You gain temporary Hit Points equal to half the negative damage the target takes (after applying resistances and the like). You lose any remaining temporary Hit Points after 1 minute.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["death","negative"],"rarity":"common","custom":""},"rules":[],"slug":"vampiric-touch","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":3},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 living creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"negative"},"value":"6d6"}}},"save":{"value":"fortitude","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":3},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":3},"heightening":{"type":"interval","interval":1,"damage":{"0":"2d6"}}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"gf0WS5OM0qEktE6P","name":"Fly","type":"spell","img":"systems/pf2e/icons/spells/fly.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.A2JfEKe6BZcTG1S8"}},"system":{"description":{"value":"<p>The target can soar through the air, gaining a fly Speed equal to its Speed or 20 feet, whichever is greater.</p>\n<hr />\n<p><strong>Heightened (7th)</strong> The duration increases to 1 hour.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"fly","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":4},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult","primal"]},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"touch"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"5 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":4,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":4}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"e24plB71990QPQdB","name":"Globe of Invulnerability","type":"spell","img":"systems/pf2e/icons/spells/globe-of-invulnerability.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.JOdOpbPDl7nqvJUm"}},"system":{"description":{"value":"<p>You create an immobile globe around yourself that attempts to counteract any spell from outside the globe whose area or targets enter into the globe, as if the globe were a <em>@Compendium[pf2e.spells-srd.9HpwDN4MYQJnW0LG]{Dispel Magic}</em> spell 1 level lower than its actual spell level.</p>\n<p>If the counteract attempt succeeds, it prevents only the portion of the spell that would have entered the globe (so if the spell also has targets outside the globe, or part of its area is beyond the globe, those targets or that area is affected normally).</p>\n<p>You must form the sphere in an unbroken open space, so its edges don't pass through any creatures or objects, or the spell is lost (though creatures can enter the globe after the spell is cast).</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"uncommon","custom":""},"rules":[],"slug":"globe-of-invulnerability","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":4},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"10 minutes"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":4},"hasCounteractCheck":{"value":true},"autoHeightenLevel":{"value":null},"areasize":{"value":"10-foot burst centered on one corner of your space"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":4}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"IlIW04nLBSm0prt9","name":"Phantasmal Killer","type":"spell","img":"systems/pf2e/icons/spells/phantasmal-killer.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.tlcrVRqW1MSKJ5IC"}},"system":{"description":{"value":"<p>You create a phantasmal image of the most fearsome creature imaginable to the target. Only the spell's target can see the killer, though you can see the vague shape of the illusion as it races forth to attack. The effect of the killer is based on the outcome of the target's Will save.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target takes 4d6 mental damage and is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 1}.</p>\n<p><strong>Failure</strong> The target takes 8d6 mental damage and is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 2}.</p>\n<p><strong>Critical Failure</strong> The target is so afraid it might die. It must attempt a Fortitude save; if the target fails, it dies. On a successful Fortitude save, the target takes 12d6 mental damage, is @Compendium[pf2e.conditionitems.sDPxOjQ9kx2RZE8D]{Fleeing} until the end of its next turn, and is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 4}. This effect has the incapacitation trait.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6 on a failure and by 3d6 on a critical failure.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["death","emotion","fear","mental"],"rarity":"common","custom":""},"rules":[],"slug":"phantasmal-killer","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":4},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 living creature"},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"mental"},"value":"8d6"}}},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":4},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":4},"heightening":{"type":"interval","interval":1,"damage":{"0":"2d6"}}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"TJn0c2CSb1tVJ92v","name":"Black Tentacles","type":"spell","img":"systems/pf2e/icons/spells/black-tentacles.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.2w4OpAGihn1JSHFD"}},"system":{"description":{"value":"<p><strong>Area</strong> 20-foot burst adjacent to a flat surface</p>\n<hr />\n<p>Oily black tentacles rise up and attempt to capture creatures in the area. Make spell attack rolls against the Fortitude DC of each creature in the area. Any creature you succeed against is @Compendium[pf2e.conditionitems.kWc1fhmv9LBiTuei]{Grabbed} and takes 3d6 bludgeoning damage. Whenever a creature ends its turn in the area, the tentacles attempt to grab that creature if they haven't already, and they deal [[/r {1d6}[bludgeoning]]]{1d6 Bludgeoning Damage} to any creature already Grabbed.</p>\n<p>The tentacles' Escape DC is equal to your spell DC. A creature can attack a tentacle in an attempt to release its grip. Its AC is equal to your spell DC, and it is destroyed if it takes 12 or more damage. Even if destroyed, additional tentacles continue to grow in the area until the duration ends. You can Dismiss the spell.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"black-tentacles","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":5},"spellType":{"value":"attack"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"conjuration"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":"20","areaType":"burst"},"time":{"value":"3"},"duration":{"value":"1 minute"},"damage":{"value":{"0":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"bludgeoning"},"value":"3d6"}}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":5},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"20-foot burst adjacent to a flat surface"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":5}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"LmUoMi4RIHuypY8J","name":"Shadow Blast","type":"spell","img":"systems/pf2e/icons/spells/shadow-blast.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.IqJ9URobmJ9L9UBG"}},"system":{"description":{"value":"<p>You shape the quasi-real substance of the Shadow Plane into a blast. Choose acid, bludgeoning, cold, electricity, fire, force, piercing, slashing, or sonic damage, and choose a 30-foot cone, a 15-foot burst within 120 feet, or a 50-foot line. The blast deals 5d8 damage of the type you chose to each creature in the area.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d8.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["shadow"],"rarity":"common","custom":""},"rules":[],"slug":"shadow-blast","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":5},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"varies"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"untyped"},"value":"5d8"}}},"save":{"value":"","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":5,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"varies"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":5},"heightening":{"type":"interval","interval":1,"damage":{"0":"1d8"}}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"vW5Z4fQ6XpzidQoq","name":"Synesthesia","type":"spell","img":"systems/pf2e/icons/spells/synesthesia.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.OAt2ZEns1gIOCgrn"}},"system":{"description":{"value":"<p>The target's senses are suddenly rewired in unexpected ways, causing them to process noises as bursts of color, smells as sounds, and so on. This has three effects, and the target must attempt a Will save. Due to the distraction, the target must succeed at a DC 5 flat check each time it uses a concentrate action, or the action fails and is wasted. The target's difficulty processing visual input makes all creatures and objects @Compendium[pf2e.conditionitems.DmAIPqOBomZ7H95W]{Concealed} from it. The creature has trouble moving, making it @Compendium[pf2e.conditionitems.i3OJZU2nk64Df3xm]{Clumsy 3} and giving it a -10-foot status penalty to its Speeds.</p>\n<hr /><strong>Critical Success</strong> The target is unaffected.\n<p><strong>Success</strong> The target is affected for 1 round.</p>\n<p><strong>Failure</strong> The target is affected for 1 minute.</p>\n<p><strong>Critical Failure</strong> As failure, and the target is @Compendium[pf2e.conditionitems.dfCMdR4wnpbYNTix]{Stunned 2} as it attempts to process the sensory shifts.</p>\n<hr />\n<p><strong>Heightened (9th)</strong> You can target up to five creatures.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["mental"],"rarity":"common","custom":""},"rules":[],"slug":"synesthesia","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":5},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["occult"]},"school":{"value":"divination"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":5},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":5},"heightening":{"levels":{"9":{"target":{"value":"5 creatures"}}},"type":"fixed"}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"E2nb9fN5Rnwitt41","name":"Dominate","type":"spell","img":"systems/pf2e/icons/spells/dominate.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.OsOhx3TGIZ7AhD0P"}},"system":{"description":{"value":"<p>You take command of the target, forcing it to obey your orders. If you issue an obviously self-destructive order, the target doesn't act until you issue a new order. The effect depends on its Will save.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target is @Compendium[pf2e.conditionitems.dfCMdR4wnpbYNTix]{Stunned 1} as it fights off your commands.</p>\n<p><strong>Failure</strong> You control the target. It gains the @Compendium[pf2e.conditionitems.9qGBRpbX9NEwtAAr]{Controlled} condition, but it can attempt a Will save at the end of each of its turns. On a success, the spell ends.</p>\n<p><strong>Critical Failure</strong> As a failure, but the target receives a new save only if you give it a new order that is against its nature, such as killing its allies.</p>\n<hr />\n<p><strong>Heightened (10th)</strong> The duration is unlimited.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["incapacitation","mental"],"rarity":"uncommon","custom":""},"rules":[],"slug":"dominate","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":6},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"enchantment"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"until the next time you make your daily preparations"},"damage":{"value":{}},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":6},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":6}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"wgGi7CHd5mQkJQ6q","name":"Phantasmal Calamity","type":"spell","img":"systems/pf2e/icons/spells/phantasmal-calamity.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.0XP2XOxT9VSiXFDr"}},"system":{"description":{"value":"<p>A vision of apocalyptic destruction fills the mind of each creature in the area. The vision deals 11d6 mental damage (basic Will save). On a critical failure, the creature must also succeed at a Reflex save or believe it's trapped (stuck in a fissure, adrift at sea, or some other fate in keeping with its vision). If it fails the second save, it's also @Compendium[pf2e.conditionitems.dfCMdR4wnpbYNTix]{Stunned} for 1 minute. It can attempt a new Will save at the end of each of its turns, and on a success, it disbelieves the illusion and recovers from the Stunned condition.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["mental"],"rarity":"common","custom":""},"rules":[],"slug":"phantasmal-calamity","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":6},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"500 feet"},"area":{"value":"30","areaType":"burst"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"mental"},"value":"11d6"}}},"save":{"value":"will","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":6,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"30-foot burst"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":6},"heightening":{"type":"interval","interval":1,"damage":{"0":"2d6"}}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"8svnxVrCmVpNHpzp","name":"Vampiric Exsanguination","type":"spell","img":"systems/pf2e/icons/spells/vampiric-exsanguination.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.fd31tAHSSGXyOxW6"}},"system":{"description":{"value":"<p>You draw in the blood and life force of other creatures through your outstretched arms. You deal 12d6 negative damage to living creatures in the area. You gain temporary Hit Points equal to half the damage a single creature takes from this spell; calculate these temporary Hit Points using the creature that took the most damage. You lose any remaining temporary Hit Points after 1 minute.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["death","negative"],"rarity":"common","custom":""},"rules":[],"slug":"vampiric-exsanguination","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":6},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"30","areaType":"cone"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"negative"},"value":"12d6"}}},"save":{"value":"fortitude","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":6},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"30-foot cone"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":6},"heightening":{"type":"interval","interval":1,"damage":{"0":"2d6"}}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"B7EeIXqd17KylaGn","name":"Ethereal Jaunt","type":"spell","img":"systems/pf2e/icons/spells/etheral-jaunt.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.D2nPKbIS67m9199U"}},"system":{"description":{"value":"<p>You travel to the Ethereal Plane, which overlaps the Material Plane. Material Plane creatures can't see you, and you can move through things on the Material Plane. You move at half your normal Speeds, but can move in any direction (including up and down). You can see onto the Material Plane within a radius of 60 feet; it is gray, hazy, and @Compendium[pf2e.conditionitems.DmAIPqOBomZ7H95W]{Concealed} from you. You can't affect the Material Plane, and you can't be affected by the Material Plane except by force effects and abjurations originating there. When the spell ends, you return to the Material Plane. If you're in the air, you fall (unless you can fly), and if you're inside an object, you're pushed into the nearest open space and take 1d6 damage per 5 feet you were pushed. If you cast this spell when not on the Material Plane, it is lost.</p>\n<hr />\n<p><strong>Heightened (9th)</strong> You can target up to five additional willing creatures at a range of 30 feet. The duration is up to 10 minutes.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["teleportation"],"rarity":"uncommon","custom":""},"rules":[],"slug":"ethereal-jaunt","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":7},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"conjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":7,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":7},"heightening":{"levels":{"9":{"range":{"value":"30 feet"},"target":{"value":"you and up to five additional willing creatures"}}},"type":"fixed"}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"a12fjECCRlhTTl7G","name":"Magnificent Mansion","type":"spell","img":"systems/pf2e/icons/spells/magnificent-mansion.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.vPWMEyVTreMOoFnm"}},"system":{"description":{"value":"<p>You conjure an extradimensional demiplane consisting of a spacious dwelling with a single entrance. The entrance connects to the plane where you Cast the Spell, appearing anywhere within the spell's range as a faint, shimmering, vertical rectangle 5 feet wide and 10 feet high. You designate who can enter when you cast the spell. Once inside, you can shut the entrance, making it @Compendium[pf2e.conditionitems.zJxUflt9np0q4yML]{Invisible}. You and the creatures you designated can reopen the door at will, just like opening a physical door.</p>\n<p>Inside, the demiplane appears to be a mansion featuring a magnificent foyer and numerous opulent chambers. The mansion can have any floor plan you imagine as you Cast the Spell, provided it fits within a space 40 feet wide, 40 feet deep, and 30 feet tall. While the entrance to the mansion is closed, effects from outside the mansion fail to penetrate it, and vice versa, except for plane shift, which can be used to enter the mansion. You can use scrying magic and similar effects to observe the outside only if they're capable of crossing planes.</p>\n<p>A staff of up to 24 servants attends to anyone within the mansion. These are like the servant created by the unseen servant spell, though they're visible, with an appearance you determine during casting. The mansion is stocked with enough food to serve a nine-course banquet to 150 people.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["extradimensional"],"rarity":"uncommon","custom":""},"rules":[],"slug":"magnificent-mansion","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":7},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"conjuration"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"1 minute"},"duration":{"value":"24 hours"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":7},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":7}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"AY3WPHgxZnc0GzDP","name":"Reverse Gravity","type":"spell","img":"systems/pf2e/icons/spells/reverse-gravity.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.37ESlJzUvVbOudOT"}},"system":{"description":{"value":"<p>You reverse gravity in the area. Creatures and objects that aren't secured to the ground immediately fall upward to the top of the area. A creature might be able to Grab an Edge to arrest its fall if it falls past an appropriate surface. If a creature falls against a solid object (such as a ceiling), it takes the appropriate amount of falling damage and lands on the surface. Once an object or creature reaches the top of the area, it floats, caught between the normal and reversed gravity. The creature can move along the plane where the two forms of gravity meet. Creatures that can levitate or fly can use those abilities to mitigate the effects of reverse gravity.When reverse gravity ends, all creatures and objects caught in the area fall back down. Likewise, anything that moves beyond the spell's area is subjected to normal gravity again.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"uncommon","custom":""},"rules":[],"slug":"reverse-gravity","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":7},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"evocation"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"3"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":7},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"20-foot radius, 40-foot-tall cylinder"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":7}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"rRdRTTkrVtfB1HbU","name":"Antimagic Field","type":"spell","img":"systems/pf2e/icons/spells/antimagic-field.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.My7FvAoLYgGDDBzy"}},"system":{"description":{"value":"<p>You repel all magic from the target area, preventing spells and other magic from functioning. Spells can't penetrate the area, magic items cease to function within it, and no one inside can cast spells or use magic abilities, including yourself. Likewise, spells - such as <em>@Compendium[pf2e.spells-srd.9HpwDN4MYQJnW0LG]{Dispel Magic}</em> - can't affect the field itself unless they are of a higher level. Magic effects resume the moment they pass outside the field. For example, a ray fired from one side of the field could target a creature on the other side (as long as caster and target are both outside the field). A summoned creature winks out of existence but reappears if the field moves or ends. Invested magic items cease to function, but they remain invested and resume functioning when they exit the field; the ability boost from an apex item isn't suppressed within the field. Spells of a higher level than the <em>antimagic field</em> overcome its effects, and can even be cast by a creature within the field.</p>\n<p>The field disrupts only magic, so a <em>+3 longsword</em> still functions as a longsword. Magically created creatures (such as golems) function normally within an <em>antimagic field</em>.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"rare","custom":""},"rules":[],"slug":"antimagic-field","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":8},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":10,"areaType":"emanation"},"time":{"value":"3"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":8},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"10-foot emanation which affects you."},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":8}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"L8sMNyCpaU6zFBgo","name":"Prismatic Wall","type":"spell","img":"systems/pf2e/icons/spells/prismatic-wall.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.iL6TujgTCtRRa0Y0"}},"system":{"description":{"value":"<p>You create an opaque wall of shimmering, multicolored light. The wall is straight and vertical, stretching 60 feet long and 30 feet high. You must form the wall in an unbroken open space so its edges don't pass through any creatures or objects, or the spell is lost. You can pass through the wall and ignore its effects. The wall sheds bright light out to 20 feet on each side (and dim light to the next 20 feet). Creatures other than you that come into the wall's light must attempt a Will save; they're @Compendium[pf2e.conditionitems.TkIyaNPgTZFBCCuh]{Dazzled} for 1 round on a success, @Compendium[pf2e.conditionitems.XgEqL1kFApUbl5Z2]{Blinded} for 1 round on a failure, and Blinded for 1 minute on a critical failure. They are then temporarily immune to the blinding effect for 1 hour.</p>\n<p>A prismatic wall has seven different layers, each a different color. Red, orange, yellow, and green have the effect of a 5th-level <em>@Compendium[pf2e.spells-srd.forsqeofEszBNtLq]{Chromatic Wall}</em> spell of that color, and the others have the effect of a 7th-level Chromatic Wall spell of that color. A creature that tries to pass through the wall must attempt a saving throw against each component wall. The effects take place simultaneously, so a creature turned to stone by the blue wall is still treated as a creature for the indigo and violet walls.</p>\n<p>The wall as a whole is immune to counteracting effects of the wall's level or lower; each color must be counteracted by its specific spell, as described in chromatic wall. This must be done in order (red, orange, yellow, green, blue, indigo, then violet). A given color can't be affected until the previous color is counteracted. Counteracting a color wall removes that color's effect from the wall, and counteracting them all ends prismatic wall. You can Dismiss the spell.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["light"],"rarity":"common","custom":""},"rules":[],"slug":"prismatic-wall","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":8},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"abjuration"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":"","areaType":""},"time":{"value":"3"},"duration":{"value":"1 hour"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":8},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":8}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"vwir5GesWQuX37XQ","name":"Spirit Song","type":"spell","img":"systems/pf2e/icons/spells/spirit-song.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Ht35SDf9PDStJfoC"}},"system":{"description":{"value":"<p>Your eldritch song sends pulsing waves of ethereal energy to attack creatures' spirits in the area, dealing 18d6 force damage. Because spirit song affects creatures' spirits, it can damage a target projecting its consciousness (such as via project image) or possessing another creature even if the target's body is elsewhere. The vibrating waves of spiritual song penetrate into, but not through, solid barriers, damaging incorporeal creatures hiding in solid objects in the area but not passing onward to damage creatures in other rooms. Possessed creatures aren't harmed by the song. The song doesn't harm creatures that have no spirit, such as constructs.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["force"],"rarity":"common","custom":""},"rules":[],"slug":"spirit-song","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":8},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"60","areaType":"cone"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"force"},"value":"18d6"}}},"save":{"value":"fortitude","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":8,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"60-foot cone"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":8},"heightening":{"type":"interval","interval":1,"damage":{"0":"2d6"}}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"jEvy2Vl9YDWXt4YU","name":"Wail of the Banshee","type":"spell","img":"systems/pf2e/icons/spells/wail-of-the-banshee.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.FEsuyf203wTNE2et"}},"system":{"description":{"value":"<p>Your scream chills the souls of enemies that hear it. Each living enemy in the area takes 8d10 negative damage and must attempt a Fortitude save.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature takes full damage.</p>\n<p><strong>Failure</strong> The creature takes full damage and is @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained} [[/r 1d4]].</p>\n<p><strong>Critical Failure</strong> The creature takes double damage and is @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 4}.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["auditory","death","negative"],"rarity":"common","custom":""},"rules":[],"slug":"wail-of-the-banshee","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":9},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"40","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"negative"},"value":"8d10"}}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":9},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"40-foot emanation"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":9}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"E5j9piT75cqydCB2","name":"Weird","type":"spell","img":"systems/pf2e/icons/spells/weird.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.qDjeG6dxT4aEEC6J"}},"system":{"description":{"value":"<p>You fill the targets' minds with terrifying images of fearsome creatures, each drawn from the targets' worst fears. Only the targets can see their assailants. Each target takes 16d6 mental damage and must attempt a Will save.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target takes half damage and is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 1}.</p>\n<p><strong>Failure</strong> The target takes full damage and is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 2}.</p>\n<p><strong>Critical Failure</strong> The target is so afraid, it might instantly die. It must attempt a Fortitude saving throw. If the target succeeds, it takes double damage and is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 2}, and it's also @Compendium[pf2e.conditionitems.sDPxOjQ9kx2RZE8D]{Fleeing} until the end of its next turn unless it critically succeeded. If it fails, it is reduced to 0 Hit Points and dies.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["death","emotion","fear","mental"],"rarity":"common","custom":""},"rules":[],"slug":"weird","schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-07T12:12:15.221+01:00","version":{"schema":0.791,"foundry":"10.287","system":"4.2.3"}}},"level":{"value":9},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"any number of creatures"},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"mental"},"value":"16d6"}}},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K3PZkEgoQbCXiKvD","heightenedLevel":9,"signature":true},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""},"heightenedLevel":{"value":9}},"ownership":{"default":0,"1ngJgIJit6FlHOOz":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"Whd0sMROjmdhgkwI","name":"Platinum Pieces","type":"treasure","img":"systems/pf2e/icons/equipment/treasure/currency/platinum-pieces.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.JuNPeK5Qm1w6wpb4"}},"system":{"description":{"value":""},"source":{"value":"Pathfinder Core Rulebook"},"trait